<!-- Hero -->
<div class="block">
  <section class="hero is-primary is-medium">
    <div class="hero-body">
      <div class="container">
        <h1 class="title is-size-1" id="heroHTML">
          ...JavaScript
        </h1>
        <h2 class="subtitle">“a.k.a. ECMAScript”

        </h2>
      </div>
    </div>
  </section>
</div>
<!-- Container for table -->
<div class="container is-fluid">
  <section class="section" id="section1">
    <div class="container has-text-centered">
      <div class="columns is-multiline">

        {{#each data}}
        <div class="card">
          <div class="card-body">
            <h5 class="card-title">{{method}}</h5>
            <p class="card-text">{{descriptions}}</p>
            <a href="/js/methods/{{id}}" class="card-link">Check me out</a>
          </div>
        </div>
        {{/each}}

      </div>
  </section>
</div>